{
  "name": "nutricombat-test",
  "version": "1.0.0",
  "main": "./App.js",
  "scripts": {
    "start": "ulimit -S -n 16384 && EXPO_USE_FAST_RESOLVER=1 expo start --port 8081",
    "start-stable": "ulimit -S -n 16384 && NODE_OPTIONS='--max-old-space-size=8192' expo start --no-dev --port 8081",
    "web": "npx expo start --web --port 3000",
    "build:web": "npx expo export:web",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "tunnel": "expo start --tunnel",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@supabase/supabase-js": "^2.57.2",
    "bcryptjs": "^3.0.2",
    "expo": "~49.0.0",
    "expo-camera": "~13.4.4",
    "expo-linear-gradient": "~12.3.0",
    "expo-status-bar": "~1.6.0",
    "node-fetch": "^2.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
